node{
def mavenhome = tool name: "maven3.9.1"
   tools
   {
   maven "maven 3.9.1"
   }
    stage("Git CheckOut"){
        git url: 'https://github.com/smuruku/spring-boot-mongo-docker.git',branch: 'master'
    }
      stage(" Build"){
     
      sh "${mavenhome}/bin/mvn clean package"
    } 
    }
